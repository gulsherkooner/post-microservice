{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/posts",
      "dest": "index.js"
    },
    {
      "src": "/posts/:post_id",
      "dest": "index.js"
    },



    {
      "src": "/posts/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/posts/:post_id/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://api-gateway-sooty-nine.vercel.app, http://localhost:3001, https://api-gateway-sooty-nine.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Max-Age": "86400"
      }
    }
  ],
  "functions": {
    "api/*": {
      "maxDuration": 3600,
      "memory": 1024
    },
    "/*": {
      "maxDuration": 3600,
      "memory": 1024
    },
    "/posts/*": {
      "maxDuration": 3600,
      "memory": 1024
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}